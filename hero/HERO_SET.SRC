		XDEF	SET_TABLE

		XDEF	STOP_SET,WALK_SET,WALKB_SET,RUN_SET,RUN_BACK_SET
		XDEF	TO_SIT_SET,TO_UP_SET,SIT_GARD_SET,SIT_SET
		XDEF	JUMP_ACT_SET,FALL_SET,JATTACK_SETA,JATTACK_SETB
		XDEF	GARD_SET,STARTC_SET,WIN_CH_SET,LOS_CH_SET
		XDEF	RIKI_SET,COM_SET,LANDING_SET,FURA_SET
		XDEF	NAGERARE_SET,STARTD_SET

		XREF	?A5
;by MOVE_CHG
		XREF	ATTACK_A_CHG,ATTACK_B_CHG
		XREF	ATTACK_SET
		XREF	ATTACK_SETA
		XREF	ATTACK_SETB
		XREF	ATTACK_SET_HAOH
