CHG		MACRO	NAME,PROG,ACT_CODE,COUNTER
NAME		EQU	(*-BASE)/8
		DC.L	PROG
		DC.W	C_\3,COUNTER
		ENDM

CONV		MACRO	CODE
		DC.B	CODE
		ENDM

NON		MACRO
		DC.B	0FFH
		ENDM





