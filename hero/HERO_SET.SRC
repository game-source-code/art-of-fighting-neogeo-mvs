STOP_SET:
		CLR.B	BEFOR_PUSH(A6)
		CLR.B	RIKI_TIMER(A6)
		CMP.W	#32,HERO_LIFE(A6)
		BCC.S	STOP_SET_0

		ADDQ.W	#8,A0
STOP_SET_0:
		JSR	CTRL_SET(PC)

		MOVE.B	#HERO_STOP_STEP,STEP1(A6)
		MOVE.B	ACT_GROUP(A6),D0
		CMP.B	#CG_WALK,D0
		BEQ.S	STOP_SET_1

		CMP.B	#CG_WALKB,D0
		BNE.S	STOP_SET_3
STOP_SET_1:
		BTST.B	#6,ACT_FLAG(A4)
		BEQ.S	STOP_SET_3

		MOVE.B	#0,MASTER_REQUEST(A4)
		MOVEQ.L	#1,D0
		RTS
STOP_SET_3:
		MOVE.B	#CG_STOP,ACT_GROUP(A6)
		MOVEQ.L	#1,D0
		RTS


WALK_SET:
		MOVE.B	#CG_WALK,ACT_GROUP(A6)
		MOVE.W	#CTRL_WALK,ACT_CTRL(A6)
		MOVE.B	#WALK_STEP,STEP1(A6)
CTRL_SET:
		MOVE.W	REG_D4(A5),ACT_CTRL(A6)
		MOVE.W	(A0),HERO_ACT_No(A6)
		MOVE.B	3(A0),COUNTER_LEVEL(A6)
		MOVE.B	#1,MASTER_REQUEST(A4)
		CLR.B	CTRL_FLAG(A6)
		MOVEQ.L	#1,D0
		RTS


WALKB_SET:
		MOVE.B	#CG_WALKB,ACT_GROUP(A6)
		MOVE.B	#BACK_STEP,STEP1(A6)
		JMP	CTRL_SET(PC)

RUN_SET:
		BTST.B	#BPS_COM_FLAG,PLAYER_OFFSET+PLAYER_STATE(A6)
		BNE.S	RS_2

		MOVEQ.L	#4,D0
		BTST.B	#ACTAT_FLIP,ACT_ATTR(A4)
		BNE	RS_1

		MOVEQ.L	#8,D0
RS_1:
		MOVE.B	D0,RUNNING_LEVER(A6)
RS_2:
		MOVE.B	#CG_RUN,ACT_GROUP(A6)
		MOVE.B	#RUN_STEP,STEP1(A6)
RS_222:
		MOVE.W	(A0),HERO_ACT_No(A6)
		MOVE.B	3(A0),COUNTER_LEVEL(A6)
		MOVE.W	REG_D4(A5),D0
		CMP.W	ACT_CTRL(A6),D0
		BEQ.S	RS_333

		MOVE.B	#1,MASTER_REQUEST(A4)
		MOVE.W	D0,ACT_CTRL(A6)
RS_333:
		CLR.B	CTRL_FLAG(A6)
		MOVEQ.L	#1,D0
		RTS


RUN_BACK_SET:
		BTST.B	#BPS_COM_FLAG,PLAYER_OFFSET+PLAYER_STATE(A6)
		BNE.S	RBS_2

		MOVEQ.L	#8,D0
		BTST.B	#ACTAT_FLIP,ACT_ATTR(A4)
		BNE	RBS_1

		MOVEQ.L	#4,D0
RBS_1:
		MOVE.B	D0,RUNNING_LEVER(A6)
RBS_2:
		MOVE.B	#CG_RUNB,ACT_GROUP(A6)
		MOVE.B	#RUNB_STEP,STEP1(A6)
		JMP	RS_222(PC)

TO_SIT_SET:
		MOVE.B	#CG_TO_SIT,ACT_GROUP(A6)
		MOVE.B	#TO_SIT_STEP,STEP1(A6)
		MOVE.W	PLAYER_No+PLAYER_OFFSET(A6),D0
		BSET.B	D0,BACK2+MOVE_DIR(A5)
		BSET.B	#BHS_DOWN,HERO_STATE(A6)
		JMP	CTRL_SET(PC)

SIT_SET:
		MOVE.B	#CG_SIT,ACT_GROUP(A6)
		MOVE.B	#SIT_STEP,STEP1(A6)
		MOVE.W	PLAYER_No+PLAYER_OFFSET(A6),D0
		BSET.B	D0,BACK2+MOVE_DIR(A5)
		BSET.B	#BHS_DOWN,HERO_STATE(A6)
		MOVE.B	#ATTACK_TIME1,ATTACK_TIMER(A6)
		JMP	CTRL_SET(PC)

TO_UP_SET:
		MOVE.B	#CG_TO_UP,ACT_GROUP(A6)
		MOVE.B	#TO_UP_STEP,STEP1(A6)
		MOVE.W	PLAYER_No+PLAYER_OFFSET(A6),D0
		BCLR.B	D0,BACK2+MOVE_DIR(A5)
		BCLR.B	#BHS_DOWN,HERO_STATE(A6)
		JMP	CTRL_SET(PC)

SIT_GARD_SET:
		MOVE.B	#CG_SITG,ACT_GROUP(A6)
		MOVE.B	#SIT_STEP,STEP1(A6)
		BSET.B	#BHS_DEFENCE,HERO_STATE(A6)
		JMP	CTRL_SET(PC)

JUMP_ACT_SET:
		MOVE.B	#ATTACK_TIME1,ATTACK_TIMER(A6)
		MOVE.B	#CG_JUMP,ACT_GROUP(A6)
		MOVE.B	#JUMP_STEP,STEP1(A6)
		BSET.B	#BHS_JUMP,HERO_STATE(A6)
		BSET.B	#6,ACT_FLAG(A4)		jump move disable
		JMP	CTRL_SET(PC)

FALL_SET:
		MOVE.B	#ATTACK_TIME1,ATTACK_TIMER(A6)
		MOVE.B	#CG_FALL,ACT_GROUP(A6)
		MOVE.B	#FALL_STEP,STEP1(A6)
		BSET.B	#BHS_JUMP,HERO_STATE(A6)
		JMP	CTRL_SET(PC)
LANDING_SET:
		MOVE.B	#ATTACK_TIME1,ATTACK_TIMER(A6)
		MOVE.B	#CG_LANDING,ACT_GROUP(A6)
		MOVE.B	#FALL_STEP,STEP1(A6)
		BCLR.B	#BHS_JUMP,HERO_STATE(A6)
		JMP	CTRL_SET(PC)


JATTACK_SETA:
		BTST.B	#BHS_JUMP,HERO_STATE(A6)
		BNE	ATTACK_SETA

		MOVEQ.L	#0,D0
		RTS
JATTACK_SETB:
		BTST.B	#BHS_JUMP,HERO_STATE(A6)
		BNE	ATTACK_SETB

		MOVEQ.L	#0,D0
		RTS

GARD_SET:
		BSET.B	#BHS_DEFENCE,HERO_STATE(A6)
		MOVE.B	#CG_WALKG,ACT_GROUP(A6)
		MOVE.B	#BACK_STEP,STEP1(A6)
		JMP	CTRL_SET(PC)


STARTC_SET:
		BCLR.B	#BHS_AUTO,HERO_STATE(A6)
		MOVE.B	#CG_STARTC,ACT_GROUP(A6)
		MOVE.B	#STARTC_STEP,STEP1(A6)
		JMP	CTRL_SET(PC)

H2_WIN_CH_SET:
		TST.B	IN_BONUS(A5)
		BNE.S	WIN_CH_SET

		MOVEP.W	WORK_N1(A4),D0
		CLR.B	D0
		TST.W	ACT_No(A5,D0.W)
		BNE.S	WIN_CH_SET

		ADDQ.W	#8,A0
WIN_CH_SET:
		MOVE.B	#CG_WIN,ACT_GROUP(A6)
		MOVE.B	#WIN_STEP,STEP1(A6)
		JMP	CTRL_SET(PC)
LOS_CH_SET:
		MOVE.B	#CG_LOS,ACT_GROUP(A6)
		MOVE.B	#LOS_STEP,STEP1(A6)
		JMP	CTRL_SET(PC)
RIKI_SET:
		MOVE.B	#120,RIKI_TIMER(A6)
		MOVE.B	#CG_RIKI,ACT_GROUP(A6)
		MOVE.B	#RIKI_STEP,STEP1(A6)
		JMP	CTRL_SET(PC)
STARTD_SET:
		MOVE.B	#STARTD_STEP,STEP1(A6)
		JMP	CTRL_SET(PC)
COM_SET:
		MOVE.W	PLAYER_No+PLAYER_OFFSET(A6),D0
		BCLR.B	D0,BACK2+MOVE_DIR(A5)
		BCLR.B	#BHS_DOWN,HERO_STATE(A6)
		MOVE.B	#CG_COM,ACT_GROUP(A6)
		MOVE.B	#COM_STEP,STEP1(A6)
		JMP	CTRL_SET(PC)
FURA_SET:
		MOVE.W	PLAYER_No+PLAYER_OFFSET(A6),D0
		BCLR.B	D0,BACK2+MOVE_DIR(A5)
		BCLR.B	#BHS_DOWN,HERO_STATE(A6)
		MOVE.B	#CG_FURA,ACT_GROUP(A6)
		MOVE.B	#FURA_STEP,STEP1(A6)
		MOVE.W	#135,FURA_TIMER(A6)
		MOVE.B	#20,FURA_LEVER(A6)
		JMP	CTRL_SET(PC)

NAGERARE_SET:
		MOVE.L	ANOTHER_PLAYER(A6),A0
		MOVEP.W	WORK_N0(A0),D0
		CLR.B	D0
		MOVEQ.L	#1,D1
		AND.B	ACT_ATTR(A5,D0.W),D1
		MOVE.B	#11111110B,D0
		AND.B	ACT_ATTR(A4),D0
		OR.B	D1,D0
		MOVE.B	D0,ACT_ATTR(A4)
		MOVE.B	#CG_NAGERARE,ACT_GROUP(A6)
		MOVE.W	HERO_TYPE(A0),D0
		MOVE.B	NAGE_TYPE(PC,D0.W),D0
		ADD.W	HERO_TYPE(A6),D0
		ADD.W	D0,D0
		MOVE.W	nage_ACT(PC,D0.W),HERO_ACT_No(A6)
		MOVE.B	#1,MASTER_REQUEST(A4)
		MOVE.B	#NAGERARE_STEP,STEP1(A6)
		MOVE.B	#-1,DOWN_DMG(A6)
		MOVEQ.L	#1,D0
		RTS

NAGE_TYPE:
		DC.B	0,10,0,20,0,0,0,0,0,0
